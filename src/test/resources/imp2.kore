      kcell{}(
        arrow{}(
          asK{AExp{}}(idAsAExp{}(X:Id{})),
          asK{AExp{}}(intAsAExp{}(I:Int{}))))),
      statecell{}(
        merge{Id{},Int{}}(
        merge{Id{},Int{}}(
          DOT2:Map{Id{},Int{}},
          bind{Id{},Int{}}(X:Id{}, I:Int{})),
          DOT3:Map{Id{},Int{}})))
  []
  */
  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{AExp{}}(idAsAExp{}(X:Id{})),
            DOT1:K{})),
        statecell{}(
          M:Map{Id{},Int{}}))),
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{AExp{}}(
              intAsAExp{}(lookup{Id{},Int{}}(M:Map{Id{},Int{}}, X:Id{}))),
            DOT1:K{})),
        statecell{}(
          M:Map{Id{},Int{}}))))
  []

  /* rule I1 / I2 => I1 /Int I2 requires I2 =/=Int 0 */

  /*
  axiom{S}
    \implies{S}(
      \equals{Bool{},S}(true{}(),
        neqInt{}(I2:Int{}, zero{}())),
      app{AExp{},S}(
        C:Ctxt{AExp{},S},
        \rewrites{AExp{}}(
          divAExp{}(intAsAExp{}(I1:Int{}), intAsAExp{}(I2:Int{})),
          intAsAExp{}(divInt{}(I1:Int{}, I2:Int{})))))
  []
  */

  /* <T> <k> I1 / I2 ~> REST </k> <state> M </statem> </T>
   * and I2 =/=Int 0 equals true
   * =>
   * <T> <k> I1 /Int I2 ~> REST </k> <state> M </statem> </T>
   */

  axiom{}
    \rewrites{Cfg{}}(
      \and{Cfg{}}(
        cfg{}(topcell{}(
          kcell{}(
            arrow{}(
              asK{AExp{}}(
                divAExp{}(
                  intAsAExp{}(I1:Int{}),
                  intAsAExp{}(I2:Int{}))),
              REST:K{})),
          statecell{}(M:Map{Id{},Int{}}))),
        \equals{Bool{},Cfg{}}(
          neqInt{}(I2:Int{}, zero{}()),
          true{}())),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{AExp{}}(
             intAsAExp{}(divInt{}(I1:Int{}, I2:Int{}))),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

          

  /* rule I1 + I2 => I1 +Int I2 */

/*
  axiom{S}
    app{AExp{},S}(
      C:Ctxt{AExp{},S},
      \rewrites{AExp{}}(
        plusAExp{}(intAsAExp{}(I1:Int{}), intAsAExp{}(I2:Int{})),
        intAsAExp{}(plusInt{}(I1:Int{}, I2:Int{}))))
  []
*/

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{AExp{}}(
              plusAExp{}(
                intAsAExp{}(I1:Int{}),
                intAsAExp{}(I2:Int{}))),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{AExp{}}(
             intAsAExp{}(plusInt{}(I1:Int{}, I2:Int{}))),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

  /* In K:
   *   rule -I1 => 0 -Int I1
   */

  /*
  axiom{S}
    app{AExp{},S}(
      C:Ctxt{AExp{},S},
      \rewrites{AExp{}}(
        minusAExp{}(I1:Int{}),
        intAsAExp{}(minusInt{}(zero{}(), I1:Int{}))))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{AExp{}}(
              minusAExp{}(
                I1:Int{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{AExp{}}(
             intAsAExp{}(minusInt{}(zero{}(), I1:Int{}))),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

  /* In K:
   *   rule I1 <= I2 => I1 <=Int I2
   */

  /*
  axiom{S}
    app{BExp{},S}(
      C:Ctxt{BExp{},S},
      \rewrites{BExp{}}(
        leqBExp{}(intAsAExp{}(I1:Int{}), intAsAExp{}(I2:Int{})),
        boolAsBExp{}(leqInt{}(I1:Int{}, I2:Int{}))))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{BExp{}}(
              leqBExp{}(
                intAsAExp{}(I1:Int{}),
                intAsAExp{}(I2:Int{}))),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{BExp{}}(
             boolAsBExp{}(leqInt{}(I1:Int{}, I2:Int{}))),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []



  /* In K:
   *   rule ! T => notBool T
   */

  /*

  axiom{S}
    app{BExp{},S}(
      C:Ctxt{BExp{},S},
      \rewrites{BExp{}}(
        notBExp{}(boolAsBExp{}(T:Bool{})),
        boolAsBExp{}(notBool{}(T:Bool{}))))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{BExp{}}(
              notBExp{}(
                boolAsBExp{}(T:Bool{}))),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{BExp{}}(
             boolAsBExp{}(notBool{}(T:Bool{}))),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []



  /* rule true && B => B */

  /*
  axiom{S}
    app{BExp{},S}(
      C:Ctxt{BExp{},S},
      \rewrites{BExp{}}(
        andBExp{}(boolAsBExp{}(true{}()), B:BExp{}),
        B:BExp{}))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{BExp{}}(
              andBExp{}(
                boolAsBExp{}(true{}()),
                B:BExp{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{BExp{}}(
             B:BExp{}),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

  /* rule false && _ => false */

  /*
  axiom{S}
    app{BExp{},S}(
      C:Ctxt{BExp{},S},
      \rewrites{BExp{}}(
        andBExp{}(boolAsBExp{}(false{}()), UNDERSCORE:BExp{}),
        boolAsBExp{}(false{}())))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{BExp{}}(
              andBExp{}(
                boolAsBExp{}(false{}()),
                B:BExp{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{BExp{}}(
             boolAsBExp{}(false{}())),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

  /* rule {} => . */

  /*
  axiom{S}
    app{K{},S}(
      C:Ctxt{K{},S},
      \rewrites{K{}}(asK{Block{}}(emptyBlock{}()), nothing{}()))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(blockAsStmt{}(emptyBlock{}())),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            nothing{}(),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))))
  []


  /* rule {S} => S */

  /*
  axiom{S}
    app{Stmt{},S}(
      C:Ctxt{Stmt{},S},
      \rewrites{Stmt{}}(
        blockAsStmt{}(block{}(S:Stmt{})),
        S:Stmt{}))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(blockAsStmt{}(block{}(S:Stmt{}))),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(S:Stmt{}),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))))
  []




  /* In K:
   *   rule <k> X = I:Int; => . ... </k>
   *        <state> ... X |-> (_ => I) ... </state>
   * In Kore:
   *   C[kcell((X = I => .) ~> DOT1)]
   *    [statecell(DOT2 merge X |-> (UNDERSCORE => I) merge DOT3)]
   */
 
  /*
  axiom{S}
    app{StateCell{},S}(
    app{KCell{},Ctxt{StateCell{},S}}(
      C:Ctxt{KCell{},Ctxt{StateCell{},S}},
      kcell{}(
        arrow{}(
          \rewrites{K{}}(
            asK{Stmt{}}(asgn{}(X:Id{}, intAsAExp{}(I:Int{}))),
            nothing{}()),
          DOT1:K{}))),
      statecell{}(
        merge{Id{},Int{}}(
        merge{Id{},Int{}}(
          DOT2:Map{Id{},Int{}},
          bind{Id{},Int{}}(X:Id{}, \rewrites{Int{}}(UNDERSCORE:Int{}, I:Int{}))),
          DOT3:Map{Id{},Int{}})))
  []
  */

  /* <T> <k> X = I; ~> DOT1 </k> <state> M </state> </T>
   * =>
   * <T> <k> . ~> DOT1 </k> <state> update(M, X, I) </state> </T>
   */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(
              asgn{}(X:Id{}, intAsAExp{}(I:Int{}))),
            DOT1:K{})),
        statecell{}(
          M:Map{Id{},Int{}}))),
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            nothing{}(),
            DOT1:K{})),
        statecell{}(
          update{Id{},Int{}}(
            M:Map{Id{},Int{}},
            X:Id{},
            I:Int{})))))
  []


  /* In K:
   *   rule S1:Stmt S2:Stmt => S1 ~> S2 [structural]
   * In Kore:
   *   asK(seq(S1,S2)) => asK(S1) ~> asK(S2)
   */

  /*
  axiom{S}
    app{K{},S}(
    C:Ctxt{K{},S},
      \rewrites{K{}}(
      asK{Stmt{}}(seq{}(S1:Stmt{},S2:Stmt{})),
      arrow{}(asK{Stmt{}}(S1:Stmt{}), asK{Stmt{}}(S2:Stmt{}))))
  []
  */
  
  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(
              seq{}(S1:Stmt{}, S2:Stmt{})),
            DOT1:K{})),
        statecell{}(
          M:Map{Id{},Int{}}))),
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            arrow{}(
              asK{Stmt{}}(S1:Stmt{}),
              asK{Stmt{}}(S1:Stmt{})),
            DOT1:K{})),
        statecell{}(
          update{Id{},Int{}}(
            M:Map{Id{},Int{}},
            X:Id{},
            I:Int{})))))
  []


  /* rule if (true) S else _ => S */


  /*
  axiom{S}
    app{Stmt{},S}(
      C:Ctxt{Stmt{},S},
      \rewrites{Stmt{}}(
        ite{}(boolAsBExp{}(true{}()),
              S:Block{},
              UNDERSCORE:Block{}),
        blockAsStmt{}(S:Block{})))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(
              ite{}(
                boolAsBExp{}(true{}()),
                S:Block{},
                UNDERSCORE:Block{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{Stmt{}}(
             blockAsStmt{}(S:Block{})),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []



  /* rule if (false) _ else S => S */

  /*
  axiom{S}
    app{Stmt{},S}(
      C:Ctxt{Stmt{},S},
      \rewrites{Stmt{}}(
        ite{}(boolAsBExp{}(false{}()),
              UNDERSCORE:Block{},
              S:Block{}),
        blockAsStmt{}(S:Block{})))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(
              ite{}(
                boolAsBExp{}(false{}()),
                UNDERSCORE:Block{},
                S:Block{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{Stmt{}}(
             blockAsStmt{}(S:Block{})),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []

  /* rule while (B) S => if (B) {S while (B) S} else {} [structural] */

  /*
  axiom{S}
    app{Stmt{},S}(
      C:Ctxt{Stmt{},S},
      \rewrites{Stmt{}}(
        while{}(B:BExp{}, S:Block{}),
        ite{}(B:BExp{},
              block{}(
                seq{}(blockAsStmt{}(S:Block{}),
                      while{}(B:BExp{}, S:Block{}))),
              emptyBlock{}())))
  []
  */

  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          arrow{}(
            asK{Stmt{}}(
              while{}(
                B:BExp{},
                S:Block{})),
            REST:K{})),
        statecell{}(M:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         arrow{}(
           asK{Stmt{}}(
             ite{}(
               B:BExp{},
               block{}(
                 seq{}(
                   blockAsStmt{}(S:Block{}),
                   while{}(B:BExp{}, S:Block{}))),
               emptyBlock{}())),
           REST:K{})),
       statecell{}(M:Map{Id{},Int{}}))))
  []
    
  /* rule <k> int (X,Xs => Xs);_ </k>
          <state> Rho:Map (.Map => X |-> 0) </state>
     requires notBool (X in keys(Rho))
   */

  /*

  axiom{S}
    \implies{S}(
      \equals{Bool{},S}(
        notBool{}(inSet{Id{}}(X:Id{}, keys{Id{},Int{}}(Rho:Map{Id{},Int{}}))),
        true{}()),
      app{StateCell{},S}(
      app{KCell{},Ctxt{StateCell{},S}}(
        C:Ctxt{KCell{},Ctxt{StateCell{},S}},
        kcell{}(
          asK{Pgm{}}(
            pgm{}(
              \rewrites{List{Id{}}}(
                cons{Id{}}(X:Id{}, Xs:List{Id{}}),
                Xs:List{Id{}}),
              UNDERSCORE:Stmt{})))),
        statecell{}(
          merge{Id{},Int{}}(
            Rho:Map{Id{},Int{}},
            \rewrites{Map{Id{},Int{}}}(
              emp{Id{},Int{}}(),
              bind{Id{},Int{}}(X:Id{}, zero{}()))))))
  []  

  */

  /* <T> <k> int (X, Xs); S </k> <state> Rho </statem> </T>
   * and notBool(X in keys(Rho)) == true
   * =>
   * <T> <k> int Xs; S </k> <state> insert(Rho, X, 0) </statem> </T>
   */

 axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          asK{Pgm{}}(
            pgm{}(
              cons{Id{}}(X:Id{}, Xs:List{Id{}}),
              UNDERSCORE:Stmt{}))),
        statecell{}(Rho:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         asK{Pgm{}}(
           pgm{}(
             Xs:List{Id{}},
             UNDERSCORE:Stmt{}))),
       statecell{}(
         insert{Id{},Int{}}(
           Rho:Map{Id{},Int{}},
           X:Id{},
           zero{}())))))
  []

  /* rule int .Ids; S => S */

  /* From type inference, the only common sort of the l.h.s. (Pgm)
   * and the r.h.s. (Stmt) is K, so this rewrite rule has sort K.
   */

  /*
  axiom{S}
    app{K{},S}(
      C:Ctxt{K{},S},
      \rewrites{K{}}(
        asK{Pgm{}}(pgm{}(nil{Id{}}(), S:Stmt{})),
        asK{Stmt{}}(S:Stmt{})))
  []
  */
    
  axiom{}
    \rewrites{Cfg{}}(
      cfg{}(topcell{}(
        kcell{}(
          asK{Pgm{}}(
            pgm{}(
              nil{Id{}}(),
              UNDERSCORE:Stmt{}))),
        statecell{}(Rho:Map{Id{},Int{}}))),
     cfg{}(topcell{}(
       kcell{}(
         asK{Stmt{}}(
           UNDERSCORE:Stmt{})),
       statecell{}(Rho:Map{Id{},Int{}}))))
  []

endmodule []
