      app{K{},K{}}(
        gamma{K{},K{}}(
          HOLE:K{},
          arrow{}(HOLE:K{}, K2:K{})),
        K1:K{}))
  []

  axiom{S,R}
    \equals{K{},R}(
      asK{S}(T:S),
      app{S,K{}}(
        gamma{S,K{}}(
          HOLE:S,
          asK{S}(HOLE:S)),
        T:S))
  []

  /** Convert contexts to arrows.
    * asK(C[T]) = asK(T) ~> asK(C). */
  axiom{S1,S2,R}
    \equals{K{},R}(
      asK{S2}(app{S1,S2}(C:Ctxt{S1,S2}, T:S1)),
      arrow{}(asK{S1}(T:S1), asK{Ctxt{S1,S2}}(C:Ctxt{S1,S2})))
  []

